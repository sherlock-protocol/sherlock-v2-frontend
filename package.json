    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.22",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vercel/node": "^1.15.4",
    "aws-sdk": "^2.1146.0",
    "axios": "^0.26.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "dompurify": "^2.4.0",
    "ethers": "^5.5.3",
    "html-react-parser": "^3.0.4",
    "js-cookie": "^3.0.1",
    "luxon": "^2.3.0",
    "node-sass": "^7.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "recharts": "^2.1.9",
    "showdown": "^2.1.0",
